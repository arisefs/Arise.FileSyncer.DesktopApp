{
    "name": "arise-filesyncer-ui",
    "version": "1.0.0",
    "description": "Arise File Syncer UI",
    "author": "Csányi István",
    "main": "dist/main/main.js",
    "scripts": {
        "start": "electron .",
        "build": "webpack",
        "watch": "webpack --watch",
        "build-prod": "webpack --config webpack.prod.config.js",
        "package:win32": "cross-var electron-packager dist $npm_package_name-$npm_package_version --asar --out=packages --platform=win32 --arch=ia32 --overwrite ",
        "package:win64": "cross-var electron-packager dist $npm_package_name-$npm_package_version --asar --out=packages --platform=win32 --arch=x64 --overwrite ",
        "package:linux": "cross-var electron-packager dist $npm_package_name-$npm_package_version --asar --out=packages --platform=linux --arch=x64 --overwrite "
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@types/node-ipc": "^9.1.1",
        "@types/react": "^16.8.7",
        "@types/react-dom": "^16.8.2",
        "babel-loader": "^8.0.5",
        "copy-webpack-plugin": "^4.6.0",
        "cross-var": "^1.1.0",
        "css-loader": "^2.1.1",
        "electron": "^4.0.8",
        "electron-packager": "^13.1.1",
        "file-loader": "^3.0.1",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.2.3",
        "typescript": "^3.3.3333",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3"
    },
    "dependencies": {
        "node-ipc": "^9.1.1",
        "react": "^16.8.4",
        "react-dom": "^16.8.4"
    }
}
